@model Dictionary<string, double>

<h2>Aggregation</h2>

<table class="table">
    <thead>
        <tr>
            <th>Question</th>
            <th>Average Answer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var entry in Model)
        {
            <tr>
                <td>@entry.Key</td>
                <td>@entry.Value</td>
            </tr>
        }
    </tbody>
</table>

<a href="/Template/Index" class="btn btn-secondary">Back to List</a>
